package DP;

public class MazePath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int n = 2;
		
		System.out.println(MazeP(0, 0, n, n));

	}
	public static int MazeP(int cr,int cc,int er,int ec)
	{
		if(cr == er&&cc == ec)
		{
			return 1;
		}
		else if(cr > er||cc > ec)
		{
			return 0;
		}
		int ch = MazeP(cr,cc+1,er,ec);
		int cv = MazeP(cr+1,cc,er,ec);
		return ch+cv;
	}

}
